	
sqlite3 side:
create table test (id integer, x integer, y integer);
.separator ","
.import DaysInHospital_Y3.csv   hh
.export hh
.save hh

myddas side:
use_module(library(myddas)).
db_open(sqlite3,con,'.hh',x,x).
db_close(con).

test 2:

use_module(library(myddas)).
assert((
t2 :-
db_open(sqlite3,con,'hh',x,x),
db_import(con,test,test),
